<script>
    function loadScript(url, callback) {
        const script = document.createElement('script');
        script.src = url;
        script.async = true;
        script.onload = callback;
        script.onerror = () => console.error(`Failed to load ${url}`);
        document.head.appendChild(script);
    }

    loadScript('https://appleid.cdn-apple.com/appleauth/static/jsapi/appleid/1/en_US/appleid.auth.js', function() {
        AppleID.auth.init({
            clientId: "com.digitalextremes.warframemobile.public",
            redirectURI: "https://www.warframe.com/buildiosuser",
            state: "CEREZlMD2CRS3Wy5",
            usePopup: true,
            responseType: "code id_token",
        });
    });

    window.signInWithApple = async () => {
        try {
            const data = await AppleID.auth.signIn()

            window.location.href = "https://www.warframe.com/buildiosuser"
                + "?state=" + data.authorization.state
                + "&code=" + data.authorization.code
                + "&id_token=" + data.authorization.id_token
        } catch ( error ) {
            //
        }
    }
</script>
<script>
    document.addEventListener('DOMContentLoaded', () => {
        window.Warframe = window.Warframe || {};

        const documentBody = document.querySelector('body');
        const authModal = document.getElementById('auth-modal');
        const authModalCloser = document.getElementById('auth-close');
        const authSignupPage = document.getElementById('auth-signup');
        const authSigninPage = document.getElementById('auth-signin');

        document.querySelectorAll('.iosBtn').forEach(function(el) {
            el.addEventListener('click', function(e) {
                e.preventDefault();
                window.signInWithApple();
            });
        });

        function closeAuthModalPages() {
            authSignupPage.classList.toggle('is-active', false);
            authSigninPage.classList.toggle('is-active', false);
        }
        function closeAuthModal(e){
            e && e.preventDefault();
            authModal.classList.toggle('is-active', false);
            documentBody.classList.toggle('scroll-disabled', false);
            const captchaBadge = document.querySelector('.grecaptcha-badge');
            if (captchaBadge) captchaBadge.style.display = 'none';
        }

        authModalCloser.addEventListener('click', closeAuthModal);

        function openAuthModal() {
            authModal.classList.toggle('is-active', true);
            documentBody.classList.toggle('scroll-disabled', true);
            const captchaBadge = document.querySelector('.grecaptcha-badge');
            if (captchaBadge) captchaBadge.style.display = 'block';
        }

        function openSigninModal() {
                        if(!window.isLoggedIn) {
                closeAuthModalPages();
                authSigninPage.classList.toggle('is-active', true);
                openAuthModal();
            } else {
                window.location.href = "https://www.warframe.com/login";
            }
                    }

        function openSignupModal() {
                        if(!window.isLoggedIn) {
                closeAuthModalPages();
                authSignupPage.classList.toggle('is-active', true);
                openAuthModal();
            } else {
                window.location.href = "https://www.warframe.com/signup";
            }
                    }

        document.addEventListener('keyup', function(e) {
            if (e.key === "Escape") { // escape key maps to keycode `27`
                closeAuthModal(e);
            }
        });

        function loginLinkHandler(e) {
            e.preventDefault();
            e.stopPropagation();
                        openSigninModal();
                    }
        const loginLinks = document.querySelectorAll('.loginLink, .havAccount');
        loginLinks.forEach((link) => link.addEventListener('click', loginLinkHandler));

        function signupLinkHandler(e) {
            e.preventDefault();
            e.stopPropagation();
                        openSignupModal();
                    }
        const signupLinks = document.querySelectorAll('.signupLink, .playFreeNow, .navSignup');
        signupLinks.forEach((link) => link.addEventListener('click', signupLinkHandler));

        window.Warframe.triggerLoginLightbox = openSigninModal;
        window.Warframe.triggerSignupLightbox = openSignupModal;
    });
</script>
