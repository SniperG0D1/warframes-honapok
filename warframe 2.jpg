    <script>
        function disableRecaptchaFormButtons(disable = true) {
            document.querySelectorAll(".uses-recaptcha").forEach(function(elem) {
                elem.disabled = disable;
            });
        }

        document.addEventListener("DOMContentLoaded", function() {
            disableRecaptchaFormButtons(true);
        });
    </script>
    <script>
        window.deRecaptchaReady = window.deRecaptchaReady || false;
        window.deRecaptchaHandlers = window.deRecaptchaHandlers || [];

        window.recaptchaReady = function() {
            grecaptcha.enterprise.ready(function() {
                window.deRecaptchaReady = true;

                disableRecaptchaFormButtons(false);

                            });
        }

        async function getToken(action) {
            await grecaptcha.enterprise.execute(
                "6LcWYwYgAAAAAIw9zG71CAPMr2oJPm3zpiaCXLVj",
                { action: action },
            ).then(function(token) {
                (window.deRecaptchaHandlers[action] || []).forEach(function(id) {
                    document.querySelector("#recaptcha-" + id + " .g-recaptcha-response").value = token;
                });
            });
        }

        async function fetchRecaptchaToken(action) {
            if (window.deRecaptchaReady) {
                await getToken(action);
                return true;
            }

            await grecaptcha.enterprise.ready(async function () {
                await getToken(action)
            });

            return false;
        }
        </script>

